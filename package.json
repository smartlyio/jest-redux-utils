{
  "name": "jest-redux-utils",
  "version": "0.0.1",
  "description": "Utilities for testing Redux reducers with Jest snapshots",
  "main": "dist/index.js",
  "repository": "git@github.com:smartlyio/jest-redux-utils.git",
  "author": "Sami Kukkonen <sami@smartly.io>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "watch": "tsc --watch"
  },
  "devDependencies": {
    "@types/flux-standard-action": "^0.5.31",
    "@types/jest": "^21.1.8",
    "flux-standard-action": "^2.0.0",
    "jest": "^21.2.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "ts-jest": "^21.2.4",
    "typescript": "^2.6.2"
  },
  "peerDependencies": {
    "jest": "^21.2.1",
    "redux": "^3.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/test/.+\\.test\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
